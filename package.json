{
  "name": "lazyapps-demo",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "pnpm-update": "pnpm --recursive update --latest",
    "test:e2e": "./tests/e2e/run.sh",
    "use-branch": "node scripts/use-branch-packages.js",
    "use-released": "node scripts/use-released-packages.js",
    "orch:build": "docker compose -f packages/orchestrated/compose.yml build",
    "orch:aspire": "docker compose -f packages/orchestrated/compose.yml --profile aspire up",
    "orch:grafana": "OTLP_HOST=grafana OTLP_PORT=4317 docker compose -f packages/orchestrated/compose.yml --profile grafana up",
    "orch:jaeger": "OTLP_HOST=jaeger OTLP_PORT=4317 docker compose -f packages/orchestrated/compose.yml --profile jaeger up",
    "orch:down": "docker compose -f packages/orchestrated/compose.yml --profile aspire --profile grafana --profile jaeger down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oliversturm/lazyapps-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oliversturm/lazyapps-demo/issues"
  },
  "homepage": "https://github.com/oliversturm/lazyapps-demo#readme",
  "devDependencies": {
    "@lazyapps/log-highlight": "^0.1.1",
    "vitest": "^2.1.8"
  }
}
